    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3",
    "@sparticuz/chromium": "^131.0.1"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.731.1",
    "@aws-sdk/util-dynamodb": "^3.731.1",
    "aws-cdk-lib": "2.176.0",
    "axios": "^1.7.9",
    "constructs": "^10.0.0",
    "dotenv": "^16.4.7",
    "puppeteer-core": "^22.15.0"
